class Solution:
    def minSubarray(self, s: List[int], a: int) -> int:
        t=sum(s)
        r=t%a
        if r==0:
            return 0
        p=0
        d={0:-1}
        ans=len(s)
        for i, x in enumerate(s):
            p=(p+x)%a
            need=(p-r+a)%a
            if need in d:
                ans=min(ans,i-d[need])
            d[p]=i
        return ans if ans<len(s) else -1
